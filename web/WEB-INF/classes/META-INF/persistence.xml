<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="jsgrid-jpa" transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<class>ua.kpi.scs.entities.Url</class>
		<properties>
			<property name="eclipselink.jdbc.password" value="jsgrid" />
			<property name="eclipselink.jdbc.user" value="jsgrid" />
			<property name="eclipselink.jdbc.driver" value="com.mysql.jdbc.Driver" />
			<property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/jsgrid" />
			<property name="eclipselink.logging.level" value="INFO" />
		</properties>
	</persistence-unit>
</persistence>